{
  "name": "babel-package-manager",
  "version": "1.0.5",
  "description": "BPM: Babel Package Manager - Automagically install babel presets/plugins and save to both package.json and .eslintrc",
  "main": "index.js",
  "bin": "bin/bpm.js",
  "scripts": {
    "test": "mocha --require babel-register --require lib/__tests__/root.js **/__tests__/*.js",
    "tdd": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aghoneim92/bpm.git"
  },
  "keywords": [
    "babel",
    "plugin",
    "transform",
    "package",
    "packages",
    "manager"
  ],
  "author": "Ahmed Ghoneim <ahmed_ghoneim92@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aghoneim92/bpm/issues"
  },
  "homepage": "https://github.com/aghoneim92/bpm#readme",
  "dependencies": {
    "bluebird": "3.4.6",
    "chalk": "1.1.3",
    "colors": "1.1.2",
    "command-line-args": "3.0.4",
    "command-line-usage": "^3.0.8",
    "pretty-error": "2.0.2",
    "ramda": "0.22.1",
    "update-notifier": "1.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-umd": "^6.18.0",
    "babel-preset-env": "^1.1.4",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-sinon": "^2.8.1",
    "eslint": "^3.12.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "mocha": "^3.2.0",
    "mocha.parallel": "^0.13.0",
    "mockery": "^2.0.0",
    "randomstring": "^1.1.5",
    "sinon": "^1.17.6"
  }
}
